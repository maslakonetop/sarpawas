<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".BerandaPawasb">

    <RelativeLayout
        android:id="@+id/rtlPawas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/ivLogoPawas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:background="@drawable/logo" />

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/rtlPawas">

            <RelativeLayout
                android:id="@+id/rtlPawas1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <ImageButton
                    android:id="@+id/btnJasHar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:background="@drawable/jasaraharja"
                    android:onClick="jashar" />


                <ImageButton
                    android:id="@+id/btnDishub"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_25sdp"
                    android:layout_toEndOf="@+id/btnJasHar"
                    android:background="@drawable/dishub"
                    android:onClick="dishub" />

                <ImageButton
                    android:id="@+id/btnDpu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_35sdp"
                    android:layout_toEndOf="@+id/btnDishub"
                    android:background="@drawable/dpu"
                    android:onClick="dpu" />

                <ImageButton
                    android:id="@+id/btnDinkes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="@drawable/dinkes"
                    android:layout_below="@id/btnJasHar"
                    android:onClick="Dinkes" />

                <ImageButton
                    android:id="@+id/btnDpbd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_25sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_below="@id/btnDishub"
                    android:layout_toEndOf="@id/btnDinkes"
                    android:background="@drawable/pbpbd"
                    android:onClick="Bpbd" />

                <ImageButton
                    android:id="@+id/btnPmi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/btnDpu"
                    android:layout_marginStart="94dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@+id/btnDpbd"
                    android:background="@drawable/pmi"
                    android:onClick="pmi" />

                <ImageButton
                    android:id="@+id/btnFatimah"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/btnDinkes"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginTop="29dp"
                    android:background="@drawable/rsifatimah"
                    android:onClick="Fatimah" />


                <ImageButton
                    android:id="@+id/btnRsu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/btnDpbd"
                    android:layout_marginStart="36dp"
                    android:layout_marginTop="20dp"
                    android:layout_toEndOf="@+id/btnFatimah"
                    android:background="@drawable/logorsud"

                    android:onClick="rsud" />

                <ImageButton
                    android:id="@+id/btnRspc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/btnPmi"
                    android:layout_marginStart="47dp"
                    android:layout_marginTop="37dp"
                    android:layout_toEndOf="@+id/btnRsu"
                    android:background="@drawable/pertamedika"
                    android:onClick="rspc" />

                <ImageButton
                    android:id="@+id/btnDamkar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_35sdp"
                    android:background="@drawable/damkar"
                    android:layout_below="@id/btnFatimah"
                    android:onClick="Damkar" />

                <ImageButton
                    android:id="@+id/btnSatpolpp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/btnRsu"
                    android:layout_marginStart="87dp"
                    android:layout_marginTop="40dp"
                    android:layout_toEndOf="@+id/btnDamkar"
                    android:background="@drawable/satpolpp"
                    android:onClick="Satpolpp" />

                <ImageButton
                    android:id="@+id/btnBasarnas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/btnRspc"
                    android:layout_marginStart="65dp"
                    android:layout_marginTop="91dp"
                    android:layout_toEndOf="@+id/btnSatpolpp"
                    android:background="@drawable/basarnas"
                    android:onClick="Basarnas" />

                <ImageButton
                    android:id="@+id/btnSantaMaria"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/btnSatpolpp"
                    android:layout_marginTop="61dp"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/santamaria"
                    android:onClick="SantaMaria" />

            </RelativeLayout>
    </ScrollView>



</androidx.constraintlayout.widget.ConstraintLayout>